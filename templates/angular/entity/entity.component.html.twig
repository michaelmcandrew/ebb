<mat-card [class.mat-elevation-z2]>
  {% if entity.labelField %}
  <mat-card-title>
    {{ '{{' }} {{ entity.names.lowerCamel }}.{{entity.labelField}} {{ '}}' }}
  </mat-card-title>
  <mat-card-subtitle>
    {{ entity.names.sentence }}
  </mat-card-subtitle>
  {% endif %}
  {% for field in fields %}<mat-form-field>
    <mat-label>{{ field.label }}</mat-label>
    <input matInput type="text" id="{{ field.name }}" [(ngModel)]="{{ entity.names.lowerCamel }}.{{ field.name }}" name="{{ field.name }}">
  </mat-form-field>
  {% endfor %}
</mat-card>
